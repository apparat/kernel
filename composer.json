{
  "name": "apparat/kernel",
  "description": "Apparat kernel",
  "license": "MIT",
  "authors": [
    {
      "name": "Joschi Kuphal",
      "email": "joschi@kuphal.net"
    }
  ],
  "minimum-stability": "RC",
  "require": {
    "php": ">=5.6.0",
    "vlucas/phpdotenv": "2.*",
    "level-2/dice": "2.*",
    "monolog/monolog": "1.x"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.3",
    "codeclimate/php-test-reporter": "0.*"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:apparat/kernel.git",
      "reference": "master"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apparat\\": "src/"
    },
    "files": [
      "src/Kernel/Module.php",
      "src/Kernel/Autorun.php"
    ]
  }
}
